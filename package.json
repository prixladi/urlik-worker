    "code:fix": "yarn eslint:fix && yarn prettier:fix"
  },
  "dependencies": {
    "mongoose": "^5.11.4",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.2",
    "@types/node": "^14.14.10",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.2"
  }
}
